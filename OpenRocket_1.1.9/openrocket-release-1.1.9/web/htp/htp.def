<set stableversion="1.0.0">
<set developmentversion="1.1.9">
<set version="${developmentversion}">

<def name="downloadbox">
  <div class="downloadbox">
    <a class="main" href="https://sourceforge.net/projects/openrocket/files/openrocket/${version}/OpenRocket-${version}.jar/download">
      <strong>Download now!</strong>
      <span>OpenRocket-<use version>.jar</span>
    </a>
    <span class="alternative">
      <a href="https://sourceforge.net/projects/openrocket/files/openrocket/${version}/ReleaseNotes/view">Release notes</a> |
      <if sourcelink>
      <a href="https://sourceforge.net/projects/openrocket/files/openrocket/${version}/OpenRocket-${version}-src.zip/download">Source code</a>
      <else>
      <a href="download.html">Other versions</a>
      </if>
    </span>
  </div>
</def>

<def name="contentbox">
</def>


